{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://booth.pm/**" },
        { "url": "https://*.booth.pm/**" }
      ]
    }
  ]
}
